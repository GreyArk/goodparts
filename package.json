{
  "name": "goodparts",
  "description": "An ESLint Style that only allows JavaScript the Good Parts (and \"Better Parts\") in your codebase.",
  "version": "1.2.0",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "tape test/index.js test/**/*.test.js",
    "coverage": "istanbul cover tape test/index.js test/**/*.test.js",
    "lint": "bin/cmd.js .",
    "lint:fix": "bin/cmd.js . --fix"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "eslint": "^3.9.1",
    "minimist": "^1.2.0"
  },
  "bin": "./bin/cmd.js",
  "contributors": [
    "nelsonic",
    "eliascodes",
    "jrans",
    "shouston3",
    "SimonLab"
  ]
}
